[
    #(load "kv" "io")

    (io/put "=== SOME/QUOTE Type Round-Trip Test ===\n")
    (kv/open-memory store)

    (io/put "\n--- Simple Quotes ---\n")
    (kv/set store:quote1 '42)
    (kv/set store:quote2 '"hello")
    (kv/set store:quote3 'symbol)
    
    (def retrieved1 (kv/get store:quote1))
    (def retrieved2 (kv/get store:quote2))
    (def retrieved3 (kv/get store:quote3))
    
    (assert (eq retrieved1 '42) "Quoted int should match")
    (assert (eq retrieved2 '"hello") "Quoted string should match")
    (assert (eq retrieved3 'symbol) "Quoted symbol should match")
    
    (io/put "Quoted integer stored and retrieved\n")
    (io/put "Quoted string stored and retrieved\n")
    (io/put "Quoted symbol stored and retrieved\n")

    (io/put "\n--- Nested Quotes ---\n")
    (kv/set store:double ''42)
    (kv/set store:triple '''100)
    
    (def retrieved_double (kv/get store:double))
    (def retrieved_triple (kv/get store:triple))
    
    (io/put "Double-quoted value stored and retrieved\n")
    (io/put "Triple-quoted value stored and retrieved\n")

    (io/put "\n--- Quoted Lists ---\n")
    (kv/set store:qlist '(1 2 3))
    (kv/set store:qnested '(1 (2 3) 4))
    
    (def retrieved_qlist (kv/get store:qlist))
    (def retrieved_qnested (kv/get store:qnested))
    
    (io/put "Quoted list stored and retrieved\n")
    (io/put "Quoted nested list stored and retrieved\n")

    (io/put "\n--- Complex Quoted Structures ---\n")
    (kv/set store:complex '((name "bosley") (data (1 2 3))))
    (def retrieved_complex (kv/get store:complex))
    (io/put "Complex quoted structure stored and retrieved\n")

    (io/put "\n=== All quote/some tests complete ===\n")
]

