[
    #(load "event" "io")

    (io/put "=== Real Type Test ===\n")

    (event/subscribe realTopic (fn (evt :any) :any [
        (io/put "Got real: %f\n" evt)
    ]))

    (event/publish realTopic 3.14159)
    (event/publish realTopic -2.5)
    (event/publish realTopic 0.0)
    (event/publish realTopic 1.23456789)

    (io/put "Published 4 reals\n")

    (io/put "\n=== Test complete ===\n")
]

