[
    #(load "event" "io")

    (io/put "=== Integer Type Test ===\n")

    (event/init)

    (event/subscribe intTopic (fn (evt :any) :any [
        (io/put "Got integer: %d\n" evt)
    ]))

    (event/publish intTopic 42)
    (event/publish intTopic -100)
    (event/publish intTopic 0)
    (event/publish intTopic 999999)

    (io/put "Published 4 integers\n")

    (io/put "\n=== Test complete ===\n")
]

