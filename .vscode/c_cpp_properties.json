{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/pkg/**",
                "${workspaceFolder}/extern/snitch/include/**",
                "${workspaceFolder}/extern/json/include/**",
                "${workspaceFolder}/extern/cpp-httplib/**",
                "/opt/homebrew/include/**",
                "/opt/homebrew/Cellar/brotli/1.1.0/include/**",
                "/opt/homebrew/Cellar/openssl@3/3.5.0/include/**",
                "/usr/local/include/**",
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include/c++/v1/**",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/17/include/**",
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include/**",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/**",
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks/**"
            ],
            "defines": [
                "BUILD_HASH=\"e03beda\"",
                "EXTRA_DEBUG=0",
                "FMT_SHARED",
                "CPPHTTPLIB_BROTLI_SUPPORT",
                "CPPHTTPLIB_OPENSSL_SUPPORT",
                "CPPHTTPLIB_USE_CERTS_FROM_MACOSX_KEYCHAIN",
                "CPPHTTPLIB_USE_NON_BLOCKING_GETADDRINFO",
                "CPPHTTPLIB_ZLIB_SUPPORT",
                "CPPHTTPLIB_ZSTD_SUPPORT"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-clang-arm64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
