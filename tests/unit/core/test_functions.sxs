[
    (set add-two (fn (a :int b :int) :int [
        (debug "Inside add-two:" a b)
        (set result-placeholder 0)
    ]))
    
    (debug "Function defined, type check")
    
    (set simple-fn (fn () :int [
        (debug "No params function")
        (set ret 42)
    ]))
    
    (debug "Calling simple-fn")
    (simple-fn)
    
    (debug "Calling add-two with args")
    (add-two 10 20)
    
    (set with-return (fn (x :int) :int [
        (debug "Param value:" x)
        (set doubled 999)
    ]))
    
    (with-return 5)
    
    (set multi-param (fn (a :int b :real c :symbol) :int [
        (debug "Multi params:" a b c)
        (set done 1)
    ]))
    
    (multi-param 100 3.14 test-symbol)
]

