[
    (def int-only (fn (x :int) :int [
        (debug "Int param:" x)
        (def res 1)
    ]))
    
    (debug "Calling with correct type - integer")
    (int-only 42)
    
    (def real-only (fn (x :real) :real [
        (debug "Real param:" x)
        (def res 1.0)
    ]))
    
    (debug "Calling with correct type - real")
    (real-only 3.14)
    
    (def symbol-only (fn (s :symbol) :symbol [
        (debug "Symbol param:" s)
        (def res test)
    ]))
    
    (debug "Calling with correct type - symbol")
    (symbol-only my-symbol)
    
    (def str-only (fn (s :str) :str [
        (debug "String param:" s)
        (def res "ok")
    ]))
    
    (debug "Calling with correct type - string")
    (str-only "hello world")
    
    (def multi-typed (fn (i :int r :real s :symbol st :str) :int [
        (debug "All types:" i r s st)
        (def res 0)
    ]))
    
    (debug "Calling with all correct types")
    (multi-typed 100 2.718 test-sym "test string")
]

