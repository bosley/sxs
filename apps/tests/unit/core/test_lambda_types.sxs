[
  (def add (fn (a :int b :int) :int [
    42
  ]))
  
  (def concat (fn (s1 :str s2 :str) :str [
    "result"
  ]))

  (def no-params (fn () :int [
    100
  ]))
  
  (def result1 (match add
    (add "matched same lambda!")
    (concat "different lambda")))
  
  (def result2 (reflect add
    (:fn<int,int>int "int->int function")
    (:fn<str,str>str "str->str function")))

  (def result3 (reflect concat
    (:fn<int,int>int "int->int function")
    (:fn<str,str>str "str->str function")))

  (def result4 (reflect no-params
    (:fn<>int "no-param function")
    (:fn<int>int "one-param function")))

  (def result5 (match concat
    (add "wrong lambda")
    (concat "correct lambda!")))
]













