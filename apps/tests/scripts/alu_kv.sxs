    (def y (kv/get mem:y))

    (io/put "x:\t%d\ny:\t%d\nz:\t%d\n" x (kv/get mem:y) (kv/get mem:z))

    (io/put "x + y:\t%d\n" (alu/mul (alu/add x y) 2))

